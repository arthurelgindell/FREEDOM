<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE plist PUBLIC "-//Apple//DTD PLIST 1.0//EN" "http://www.apple.com/DTDs/PropertyList-1.0.dtd">
<plist version="1.0">
<dict>
    <key>Label</key>
    <string>com.freedom.saf-monitor</string>

    <key>ProgramArguments</key>
    <array>
        <string>/bin/bash</string>
        <string>/Volumes/DATA/FREEDOM/SAF_StudioAirFabric/saf_service_monitor.sh</string>
    </array>

    <key>WorkingDirectory</key>
    <string>/Volumes/DATA/FREEDOM</string>

    <key>UserName</key>
    <string>arthurdell</string>

    <key>RunAtLoad</key>
    <true/>

    <key>KeepAlive</key>
    <true/>

    <key>StandardOutPath</key>
    <string>/Volumes/DATA/FREEDOM/SAF_StudioAirFabric/logs/saf-monitor.log</string>

    <key>StandardErrorPath</key>
    <string>/Volumes/DATA/FREEDOM/SAF_StudioAirFabric/logs/saf-monitor-error.log</string>

    <key>StartInterval</key>
    <integer>300</integer>
</dict>
</plist>